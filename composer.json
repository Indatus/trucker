{
    "name": "indatus/trucker",
    "description": "A simple active resource implementation written in PHP to consume any REST API",
    "keywords":["trucker", "activeresource", "active resource"],
    "type": "library",
    "homepage": "http://indatus.com/",
    "license": "MIT",
    "authors": [
        {
            "name": "Brian Webb",
            "homepage": "http://indatus.com/",
            "email": "bwebb@indatus.com",
            "role": "Developer"
        }
    ],
    "repositories": [
        {
            "type": "vcs",
            "url": "https://github.com/brianwebb01/filesystem"
        },
        {
            "type": "vcs",
            "url": "https://github.com/brianwebb01/hot-phpunit-runner"
        }
    ],
    "require": {
        "php": ">=5.4.0",
        "illuminate/support": "~4",
        "illuminate/config": "~4",
        "illuminate/container": "~4",
        "guzzle/guzzle": "3.8.1",
        "doctrine/inflector": "1.0",
        "patchwork/utf8": "1.1.*"
    },
    "require-dev": {
        "mockery/mockery": "0.9.0",
        "satooshi/php-coveralls": "dev-master",
        "phpunit/phpunit": "3.7.*@dev",
        "hot/phpunit-runner": "dev-master"
    },
    "autoload": {
        "psr-0": {
            "Trucker": "src/"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.1-dev"
        }
    },
    "minimum-stability": "dev"
}
